{
  "name": "@opeoginni/github-copilot-openai-compatible",
  "version": "0.1.16",
  "description": "GitHub Copilot API OpenAI-Compatible Provider for AI SDK",
  "license": "MIT",
  "git": "https://github.com/OpeOginni/github-copilot-openai-compatible",
  "author": "Opeyemi Oginni",
  "sideEffects": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:example": "npm run build && npx tsx examples/test-usage.ts",
    "pack:test": "npm run build && npm pack"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.42",
    "@ai-sdk/openai-compatible": "^1.0.19",
    "@ai-sdk/provider": "^2.1.0-beta.4",
    "@ai-sdk/provider-utils": "^3.0.10"
  },
  "devDependencies": {
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.34.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "ai",
    "ai-sdk",
    "github",
    "copilot",
    "openai",
    "compatible",
    "llm"
  ]
}
